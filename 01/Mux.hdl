/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = sel, out = notsel);
    Nand(a = a, b = notsel, out = nand1out);
    Nand(a = sel, b = b, out = nand2out);
    Nand(a = nand1out, b = nand2out, out = out);
}